#include"bst.h"
int main()
{
	int i;
	int a[11] = { 62, 88, 58, 47, 35, 73, 51,88, 99, 37, 93 };
	bstree<int> b;
	for (i = 0; i < 11; i++)
		b.InsertBST(a[i]);
	b.DeleteBST(b.Root(),93);
	cout << "以递归方法遍历二叉树：" << endl;
	cout << "前序遍历结果：";
	b.preOrder(b.Root());
	cout << endl;
	cout << "中序遍历结果：";
	b.inOrder(b.Root());
	cout << endl;
	cout << "后序遍历结果：";
	b.postOrder(b.Root());
	cout << endl;
	return 0;
}
